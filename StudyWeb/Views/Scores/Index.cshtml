@model IEnumerable<StudyWeb.Models.Score>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Quản Lý Điểm Kiểm Tra</h2>

<table class="table">
    <tr>
        <th>
            Điểm
        </th>
        <th>
            Ngày làm bài
        </th>
        <th>
            Hiển thị
        </th>
        <th>
            Tên
        </th>
        <th>
            Tên bài kiểm tra
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.score1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.dateAdd)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.display)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Account.fullname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Test.nameTest)
        </td>
        <td>
            @Html.ActionLink("Chi tiết", "Details", new { id=item.id }) |
            @Html.ActionLink("Xóa", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
